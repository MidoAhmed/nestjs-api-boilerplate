{
  "name": "nestjs-api-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:up": "docker-compose -p nestjs-api-boilerplate up -d",
    "docker:down": "docker-compose -p nestjs-api-boilerplate down",
    "seed:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config -r src/database/config -n typeorm.config.ts",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed -r src/database/config -n typeorm.config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:show": "npm run typeorm -- migration:show --config src/database/config/typeorm.config.ts",
    "migration:generate": "npm run typeorm -- migration:generate --config src/database/config/typeorm.config.ts -n",
    "migration:run": "npm run typeorm -- migration:run --config src/database/config/typeorm.config.ts",
    "migration:revert": "npm run typeorm -- migration:revert --config src/database/config/typeorm.config.ts",
    "doc:generate": "npx compodoc -p tsconfig.json -n 'Nest api boilerplate documentation'",
    "doc:serve": "npx compodoc -s --port 9000",
    "schema:sync": "npm run typeorm -- schema:sync --config src/database/config/typeorm.config.ts",
    "schema:log": "npm run typeorm -- schema:log --config src/database/config/typeorm.config.ts",
    "schema:drop": "npm run typeorm -- schema:drop --config src/database/config/typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "aws-sdk": "^2.658.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^3.2.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "config": "^3.3.0",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "nest-winston": "^1.3.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.24",
    "typeorm-seeding": "^1.5.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.1.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^7.0.1",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.11",
    "@types/jest": "25.1.2",
    "@types/node": "^13.13.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
